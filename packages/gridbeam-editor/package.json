{
  "name": "gridbeam-editor",
  "version": "0.0.0",
  "description": "GridBeam editor using React + Regl",
  "main": "index.js",
  "scripts": {
    "start": "budo example.js --live",
    "test": "dependency-check . && dependency-check . --extra --no-dev -i babelify -i @babel/preset-env -i @jscad/csg"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  },
  "standard": {
    "globals": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/gridbeamjs/tree/master/packages/gridbeam-csg.git"
  },
  "keywords": [
    "gridbeam",
    "csg",
    "cad"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (https://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/gridbeamjs/issues"
  },
  "homepage": "https://github.com/ahdinosaur/gridbeamjs/tree/master/packages/gridbeam-csg/#readme",
  "devDependencies": {
    "@jscad/csg": "^0.7.0",
    "budo": "^11.6.2",
    "dependency-check": "^3.1.0",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "csg-to-mesh": "^1.0.2",
    "gl-mat4": "^1.2.0",
    "gridbeam-csg": "0.0.0",
    "normals": "^1.1.0",
    "react": "^16.8.6",
    "react-three-fiber": "^2.2.14",
    "rebass": "^3.1.2",
    "styled-components": "^4.3.2",
    "three": "^0.106.2"
  }
}
